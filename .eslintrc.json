{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "import/prefer-default-export": "off"
  },
  "compilerOptions": {
    "types": ["@vueuse/head"]
  }
}
